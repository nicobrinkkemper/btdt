{
  "name": "BDTD",
  "description": "Been there done that launch site",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "now-build": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "sass": "npx node-sass ./scss/global.scss --include-path ./scss/ --include-path ./node_modules -o ./static/",
    "sass-dev": "npx node-sass ./scss/global.scss --include-path ./scss/ --include-path ./node_modules -o ./static/ -w"
  },
  "dependencies": {
    "@material/form-field": "^0.43.0",
    "@material/textfield": "^0.43.1",
    "autoprefixer": "^9.4.6",
    "compression": "^1.7.3",
    "material-components-web": "^0.38.2",
    "polka": "^0.5.1",
    "sirv": "^0.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "chokidar": "^2.0.4",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^1.0.1",
    "sapper": "^0.24.2",
    "svelte": "^2.0.0",
    "svelte-preprocess": "^2.4.3",
    "svelte-preprocess-sass": "^0.1.0"
  },
  "peerDependencies": {
    "material-components-web": "^0.38.2"
  }
}
